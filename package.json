{
  "name": "zap-extension",
  "version": "0.0.12",
  "description": "ZAP by Checkmarx Browser Extension - allows ZAP to interact directly with the browser.",
  "private": true,
  "repository": "https://github.com/zaproxy/browser-extension/",
  "author": {
    "name": "ZAP Core Team",
    "email": "zaproxy-admin@googlegroups.com",
    "url": "https://zaproxy.org"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">= 1.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-node"
  },
  "scripts": {
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch",
    "build:ext:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:ext:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:ext:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "build:ext": "yarn run build:ext:chrome && yarn run build:ext:firefox && yarn run build:ext:opera",

    "build:rec:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack --config webpack.config.rec.js",
    "build:rec:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack --config webpack.config.rec.js",
    "build:rec:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack --config webpack.config.rec.js",
    "build:rec": "yarn run build:rec:chrome && yarn run build:rec:firefox && yarn run build:rec:opera",
    "build": "yarn run build:ext && yarn run build:rec",

    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --coverage --testTimeout=10000",
    "buildAndTest": "yarn run build && yarn test"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "advanced-css-reset": "^2.1.3",
    "emoji-log": "^1.0.2",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "lodash": "^4.17.21",
    "webext-base-css": "^2.1.0",
    "webextension-polyfill": "0.12.0"
  },
  "devDependencies": {
    "4.5": "^0.5.0",
    "@abhijithvijayan/eslint-config": "2.8.1",
    "@abhijithvijayan/eslint-config-airbnb": "^1.0.2",
    "@abhijithvijayan/tsconfig": "^1.3.0",
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/plugin-transform-destructuring": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.4.3",
    "@playwright/test": "^1.52.0",
    "@types/jest": "^29.4.0",
    "@types/json-server": "^0.14.8",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.19",
    "@types/text-encoding": "^0.0.40",
    "@types/webextension-polyfill": "^0.12.3",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^5.1.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^26.1.0",
    "json-server": "^0.17.3",
    "mini-css-extract-plugin": "^2.9.2",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "playwright": "1.37",
    "playwright-webextext": "^0.0.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.3.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-ext-reloader": "^1.1.9",
    "wext-manifest-loader": "^2.3.0",
    "wext-manifest-webpack-plugin": "^1.2.1"
  }
}
